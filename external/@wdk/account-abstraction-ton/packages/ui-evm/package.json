{
  "name": "@wdk-account-abstraction-ton/ui-evm",
  "version": "1.0.0-canary.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./tenderly": {
      "require": "./dist/tenderly.cjs",
      "import": "./dist/tenderly.mjs",
      "types": "./dist/tenderly.d.ts"
    },
    "./anvil": {
      "require": "./dist/anvil.cjs",
      "import": "./dist/anvil.mjs",
      "types": "./dist/anvil.d.ts"
    },
    "./typechain": {
      "require": "./dist/typechain.cjs",
      "import": "./dist/typechain.mjs",
      "types": "./dist/typechain.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "codegen": "npx typechain --target=ethers-v5 './src/abi/*.json' --out-dir ./src/typechain",
    "build": "npx tsup",
    "test": "vitest",
    "dev": "npx tsup --watch",
    "bump": "npm version prerelease --preid=canary",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm yarn.lock"
  },
  "keywords": [],
  "author": "",
  "license": "BUSL-1.1",
  "dependencies": {
    "@0xsequence/multicall": "^0.43.15",
    "@ton/crypto": "3.3.0",
    "@viem/anvil": "^0.0.9",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "p-memoize": "4.0.4",
    "redaxios": "^0.5.1",
    "stream-browserify": "^3.0.0",
    "tsup": "^6.7.0",
    "util": "^0.12.5",
    "viem": "2.22.10"
  },
  "devDependencies": {
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/node": "^22.14.0",
    "@wdk-account-abstraction-ton/tsup-config": "0.0.8",
    "@wdk-account-abstraction-ton/typescript-config": "^0.0.3",
    "@wdk-account-abstraction-ton/ui-core": "1.0.0-canary.2",
    "ethers": "^5.7.0",
    "get-port": "^7.1.0",
    "micro-memoize": "~4.1.2",
    "redaxios": "^0.5.1",
    "typechain": "^8.0.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@wdk-account-abstraction-ton/ui-core": "1.0.0-canary.2",
    "ethers": "^5.7.0",
    "get-port": "^7.1.0",
    "micro-memoize": "~4.1.2",
    "redaxios": "*"
  }
}
